    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
